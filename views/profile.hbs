<div class="container">
  <div class="row">
    <div class="offset-4 col-4">
      <div class="card text-center">
        <div class="card-header">
          <img src="{{user.thumbnail}}" alt="Profile Picture">
        </div>
        <div class="card-body">
          <h5 class="card-title">{{user.displayName}}</h5>
          <p class="card-text">{{user.email}}</p>
          <a href="/profile/home" class="btn btn-primary">Go Home, brother</a>
        </div>
      </div>
    </div>
  </div>
</div>